package ltcb;

import java.util.Scanner;

public class Project_06 {


	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("nhap vao so bat ki: ");
		int a=sc.nextInt();
		int gt=1;
		for(int i=1;i<=a;i++)
			gt*=i;
		System.out.println(gt);

	}
}